{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "transform-decorators-legacy"
  ],
  "comments": true
}